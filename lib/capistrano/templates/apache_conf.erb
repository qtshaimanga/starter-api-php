<VirtualHost *:80>
ServerAdmin  <%= fetch(:server_admin) %>
ServerName <%= fetch(:server_name) %>
ServerAlias www.<%= fetch(:server_name) %>
DocumentRoot <%= current_path %>
<Directory  <%= current_path %>>
AllowOverride none
#AllowOverride All
</Directory>
ErrorLog /var/www/watch/logs/error.log
CustomLog /var/www/watch/logs/access.log combined
</VirtualHost>
